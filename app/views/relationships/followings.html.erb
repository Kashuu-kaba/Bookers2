<h2>Followings Users</h2>
    <table class="table">
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th></th>
          <th></th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
        <tr>
          <td>
            <%= image_tag user.image.variant(resize:'300x300') %>
          </td>
          <td><%= user.name %></td>
           <td>フォロー数:<%= user.followings.count %></td>
          <td>フォロワー数:<%= user.followers.count %></td>
          <td><%= link_to "show", user_path(user)  %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
